<!-- Schema.org JSON-LD -->
<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "BreadcrumbList",
        "itemListElement": [{
            "@type": "ListItem",
            "position": 1,
            "name": "SwipeUp",
            "item": "{{ url('app_homepage') }}"
        },{
            "@type": "ListItem",
            "position": 2,
            "name": "{{ subTitle|slice(0, -3) }}",
            "item": "{{ app.request.schemeAndHttpHost }}{{ app.request.requestUri }}"
        }]
    }


</script>